@model KDSB20241103.Models.Cliente

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles</h1>

<div>
    <h4>Cliente</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NombreCliente)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NombreCliente)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.FechaRegistro)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaRegistro)
        </dd>
    </dl>
    <div class="row row-cols-md-12">
        @await Html.PartialAsync("_DetTelefonoCliente", Model.TelefonoCliente)
    </div>
</div>
<div>
    <a class="btn btn-success" asp-action="Edit" asp-route-id="@Model?.IdCliente">Editar</a> |
    <a class="btn btn-danger" asp-action="Index">Cancelar</a>
</div>
